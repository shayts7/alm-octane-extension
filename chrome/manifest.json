{
  "manifest_version": 2,
  "name": "ALM Octane Chrome Extension",
  "description": "ALM Octane Chrome Extension",
  "version": "0.0.2",
  "browser_action": {
    "default_icon": "img/octane.png",
    "default_popup": "main-popup.html",
    "default_title": "Octane Chrome Extension"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["artemis/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://localhost:8080/ui/*"],
      "js": ["artemis/content-octane.js", "prism/content-aut.js"]
    },
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["artemis/lib/artemis-core/artemis.core.js", "artemis/content-aut.js"]
    },
    {
      "matches": ["http://localhost:8080/ui/*", "http://myd-vm12623.hpeswlab.net:8081/*"],
      "js": ["prism/content-aut.js"]
    }
  ]
}
